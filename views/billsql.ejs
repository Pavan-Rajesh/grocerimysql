<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/bill.css">

</head>

<body>
    <h1>
        <% foo.forEach(element=> { %>
            <h2>
                <%=element.pid%>

            </h2>
            <% });%>
    </h1>

    <div class="container">
        <div class="row gutters">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card">
                    <div class="card-body p-0">
                        <div class="invoice-container">
                            <div class="invoice-header">
                                <!-- Row start -->
                                <div class="row gutters">
                                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12">
                                        <div class="custom-actions-btns mb-5">
                                            <a href="#" class="btn btn-primary">
                                                <i class="icon-download"></i> Download
                                            </a>
                                            <a href="#" class="btn btn-secondary">
                                                <i class="icon-printer"></i> Print
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <!-- Row end -->
                                <!-- Row start -->

                                <!-- Row end -->
                                <!-- Row start -->

                                <div class="col-xl-3 col-lg-3 col-md-12 col-sm-12 col-12">
                                    <div class="invoice-details">
                                        <div class="invoice-num">
                                            <div>Invoice - #009</div>
                                            <div>January 10th 2020</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- Row end -->
                        </div>
                        <div class="invoice-body">
                            <!-- Row start -->
                            <div class="row gutters">
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <div class="table-responsive">
                                        <table class="table custom-table m-0">
                                            <thead>
                                                <tr>
                                                    <th>Items</th>
                                                    <th>Product ID</th>
                                                    <th>Quantity</th>
                                                    <th>Sub Total</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>
                                                        Wordpress Template
                                                        <p class="m-0 text-muted">
                                                            Reference site about Lorem Ipsum, giving information on
                                                            its
                                                            origins.
                                                        </p>
                                                    </td>
                                                    <td>#50000981</td>
                                                    <td>9</td>
                                                    <td>$5000.00</td>



                                                </tr>

                                                <% foo.forEach(element=> { %>
                                                    <tr>
                                                        <td>
                                                            <%= element.pname%>
                                                        </td>
                                                        <td>
                                                            <%=element.pid%>
                                                        </td>
                                                        <td>
                                                            <%=element.quantity%>
                                                        </td>
                                                        <td>
                                                            <%=element.price%>
                                                        </td>
                                                    </tr>
                                                    <% });%>





                                                        <tr>
                                                            <td>&nbsp;</td>
                                                            <td colspan="2">
                                                                <p>
                                                                    Subtotal<br>
                                                                    Shipping &amp; Handling<br>
                                                                    Tax<br>
                                                                </p>
                                                                <h5 class="text-success"><strong>Grand
                                                                        Total</strong></h5>
                                                            </td>
                                                            <td>
                                                                <p>
                                                                    $5000.00<br>
                                                                    $100.00<br>
                                                                    $49.00<br>
                                                                </p>
                                                                <h5 class="text-success"><strong>$5150.99</strong>
                                                                </h5>
                                                            </td>
                                                        </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                            <!-- Row end -->
                        </div>
                        <div class="invoice-footer">
                            Thank you for your Business.
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>

    <button onclick="location.href = 'http://localhost:3000/logout';"> logout</button>

</body>

</html>